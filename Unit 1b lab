        Console.WriteLine("30 is greater than 20");
      }    
    }
  }
}

using System;
					
public class Program
{
	public void Main()
	{
		Console.WriteLine ("Welcome");
		DoMath(30, 4);
		DoMath(45, 7);
		DoMath(54, 10);}
	
	
	public void DoMath (int value, int value2) {
		var number = value + value;
		Console.WriteLine(number);
	}}
	
#include <iostream>
using namespace std;

int main() {
  int time = 30;
  if (time < 18) {
    cout << "Good day.";
  } else {
    cout << "Good morning.";
  }
  return 0;
}

#include <iostream>
using namespace std;

int main() {
  int day = 7;
  switch (day) {
  case 1:
    cout << "Day 1";
    break;
  case 2:
    cout << "Day 2";
    break;
  case 3:
    cout << "Day 3";
    break;
  case 4:
    cout << "Day 4";
    break;
  case 5:
    cout << "Day 5";
    break;
  case 6:
    cout << "Day 6";
    break;
  case 7:
    cout << "Day 7";
    break;
  }
  return 0;
}
